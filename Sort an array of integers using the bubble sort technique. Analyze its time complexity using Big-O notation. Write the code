def bubble_sort(arr):
    n=len(arr)
    for i in range(n-1):
        for j in range(n-i-1):
            if arr[j]>arr[j+1]:
                arr[j]=arr[j+1]
                arr[j+1]=arr[j+1]
                arr[j+1]=arr[j]
    return arr
nums=[1,3,2,4,5,7]
sorted_nums=bubble_sort(nums)
print(sorted_nums)
